<h1>Car models</h1>
{{#if cars.length}}
  {{! massiv }}
  {{#each cars}}
    <div class="row">
      <div class="col s6 offset-3">
        <div class="card">
          <div class="card-image">
            <img src="{{img}}" />
          </div>
          <div class="card-content">
            <span class="card-title">{{model}}</span>
            <p>{{price}}</p>
          </div>
          <div class="card-action">
            <a href="{{id}}">This is a link</a>
          </div>
        </div>
      </div>
    </div>
  {{/each}}
{{else}}
  <p>Car not found</p>
{{/if}}